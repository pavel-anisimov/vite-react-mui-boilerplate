{
  "name": "vite-react-mui-boilerplate",
  "version": "0.1.0",
  "private": true,
  "description": "Vite + TypeScript + React + MUI Boilerplate with ESLint, Prettier, and Biome",
  "type": "module",
  "engines": {
    "node": ">=22.19",
    "npm": ">=11.0.0"
  },
  "keywords": ["Vite", "TypeScript", "React", "MUI", "Biome", "Boilerplate"],
  "repository": {
    "type": "git",
    "url": "https://github.com/pavel-anisimov/vite-react-mui-boilerplate"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "biome format .",
    "format:check": "biome check .",
    "check": "npm run typecheck && npm run lint && npm run format:check && npm run test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.1.1",
    "react-country-flag": "^3.1.0",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.2",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.18.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.34.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^60.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "biome format --write"
    ],
    "*.{json,css,md}": [
      "biome format --write"
    ]
  }
}
